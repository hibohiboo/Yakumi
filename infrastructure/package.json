    "aws-cdk": "2.106.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.106.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  }
}
