{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ES2021"],
    "module": "commonjs",
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": false, //未使用チェックをしない
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": false //.d.tsの型チェックを有効化
  }
}