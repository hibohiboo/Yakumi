{
  "name": "@yakumi/eslint-config-custom",
  "type": "module",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.4",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.0.14",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-unused-imports": "^4.1.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0"
  },
  "scripts": {
    "inspect": "npx @eslint/config-inspector",
    "build": "npx -y @eslint/config-inspector build --base Yakumi/eslint-config-inspector  &&  rm -rf ../../docs/public/eslint-config-inspector && cp -r .eslint-config-inspector ../../docs/public/eslint-config-inspector",
    "ncu": "ncu -u"
  }
}
